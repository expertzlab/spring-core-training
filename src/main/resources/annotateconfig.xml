<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config />
    <bean name="reqBean" class="com.expertzlab.spring.core.annotationconfig.RequireBean">
        <property name="reqString" value="test" />
    </bean>
    <bean name="profile" class="com.expertzlab.spring.core.annotationconfig.Profile">

    </bean>
    <bean name="student2" class="com.expertzlab.spring.core.annotationconfig.Student">
        <property name="name" value="Gireesh" />
        <property name="age" value="32" />

    </bean>


    <bean name="student1" class="com.expertzlab.spring.core.annotationconfig.Student">
        <property name="name" value="Suhail" />
        <property name="age" value="23" />

    </bean>


    <bean name="teacher" class="com.expertzlab.spring.core.annotationconfig.Teacher">
        <property name="subject" value="Maths" />
    </bean>

    <!--
    <bean name="subject1" class="com.expertzlab.spring.core.annotationconfig.Subject">
        <constructor-arg value="Physics" />
    </bean>

    -->
    <bean name="jsr250" class="com.expertzlab.spring.core.annotationconfig.JSR250Annotations"/>

</beans>